@model IEnumerable<Games>
@{
    ViewData["Title"] = "Game Collection";
}

<h1>Game Collection</h1>
<h2>You have @ViewBag.count games in your collection</h2>
<div>
    <br /><a asp-controller="Home" asp-action="Add" class="add">Add New Game</a><br /><br />
    <div class="allGames">
        @foreach (Games games in Model)
        {
            <partial name="_GamePartialView" for="@games"/>
        }
    </div>
</div>